{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "lib/libs/logger/(.*)": "<rootDir>/../libs/libs/logger/src/$1",
    "lib/libs/logger": "<rootDir>/../libs/libs/logger/src",
    "@lib/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@lib/logger": "<rootDir>/../libs/logger/src",
    "@lib/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@lib/common": "<rootDir>/../libs/common/src"
  }
}